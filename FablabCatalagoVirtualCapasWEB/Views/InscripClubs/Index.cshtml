@model IEnumerable<FablabCatalagoVirtualCapasEN.InscripcionClub>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@section styles {
    <link rel="stylesheet" href="@Url.Content("~/Content/inscripcionClub.css")" />
}

<h2>Listado de Incripciones de Clubs</h2>

<p>
    @Html.ActionLink("Create New", "Create", null, new { @class = "btn btn-white" })
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.IdAutor)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IdClub)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FechaInscripcion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Detalle)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.IdAutor)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdClub)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FechaInscripcion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Detalle)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
                @Html.ActionLink("Details", "Details", new { id = item.Id }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.Id })
            </td>
        </tr>
    }

</table>
