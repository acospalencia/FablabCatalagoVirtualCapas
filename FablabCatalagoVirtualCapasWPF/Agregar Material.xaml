<Window x:Class="Fablab.esfe.Agregar_Material"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Fablab.esfe"
        mc:Ignorable="d"
        Title="Agregar_Material" Height="450" Width="800">
    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="/BgSelecMaterial.png"/>
        </Grid.Background>
        <Label Content="Elija la opcion a realizar" HorizontalAlignment="Center" Margin="0,72,0,0" VerticalAlignment="Top" FontWeight="Bold" FontFamily="Arial" FontSize="20" Foreground="#FFECE7E7"/>
        <Button Content="Ingresar" HorizontalAlignment="Center" Margin="0,196,0,0" VerticalAlignment="Top" FontWeight="Bold" FontFamily="Arial" FontSize="16" Height="30" Width="192" Background="#FF924CAD"/>
        <Button Content="Modificar" HorizontalAlignment="Center" Margin="0,273,0,0" VerticalAlignment="Top" FontWeight="Bold" FontFamily="Arial" FontSize="16" Height="30" Width="192" Background="#FF924CAD"/>

    </Grid>
</Window>
