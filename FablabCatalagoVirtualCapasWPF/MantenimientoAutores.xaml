<Window x:Class="interfaces.MantenimientoAutores"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:interfaces"
        mc:Ignorable="d"
        Title="MantenimientoAutores" Height="450" Width="800">
    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="/BgModiAUtor.jpg"/>
        </Grid.Background>
        <Button x:Name="btnIngresar" Content="" HorizontalAlignment="Left" Margin="255,168,0,0" VerticalAlignment="Top" Width="76" Height="69">
            <Button.Background>
                <ImageBrush ImageSource="/BgBtnIngresar.png"/>
            </Button.Background>
        </Button>
        <Button x:Name="btnModificar" Content="" HorizontalAlignment="Left" Margin="445,168,0,0" VerticalAlignment="Top" Width="84" Height="69">
            <Button.Background>
                <ImageBrush ImageSource="/BgEditar.png"/>
            </Button.Background>
        </Button>
        <Label Content="MANTENIMIENTO DE AUTORES" HorizontalAlignment="Left" Margin="255,81,0,0" VerticalAlignment="Top" FontFamily="Arial" FontWeight="Bold" FontSize="20" Foreground="White"/>
        <Button x:Name="btnRegresar" Content="" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="44" Height="32">
            <Button.Background>
                <ImageBrush ImageSource="/BgRegresar.png"/>
            </Button.Background>
        </Button>

    </Grid>
</Window>
