        <DataGrid d:ItemsSource="{d:SampleData ItemCount=5}" Margin="733,154,31,121"/>
        <Button x:Name="btnBuscar" Content="" HorizontalAlignment="Left" Margin="1080,90,0,0" VerticalAlignment="Top" Width="45" Height="35">
