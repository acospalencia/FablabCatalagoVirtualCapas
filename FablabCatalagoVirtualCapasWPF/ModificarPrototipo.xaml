                <ImageBrush ImageSource="/BgBtnGuardar.jpeg"/>
            </Button.Background>
        </Button>
        <Button x:Name="btnEliminar" Content="" HorizontalAlignment="Left" Margin="1038,406,0,0" VerticalAlignment="Top" Width="64" Height="60">
            <Button.Background>
                <ImageBrush ImageSource="/BgPapelera.png"/>
            </Button.Background>
        </Button>
        <Label Content="Autor" HorizontalAlignment="Left" Margin="72,108,0,0" VerticalAlignment="Top" Foreground="White" FontWeight="Bold" FontFamily="Arial" FontSize="16"/>
