<Window x:Class="Fablab.esfe.InicioSesion"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Fablab.esfe"
        mc:Ignorable="d"
        Title="InicioSesion" Height="450" Width="800" ResizeMode="NoResize" WindowStartupLocation="CenterScreen" Closed="Window_Closed">
    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="/BgLogin.png"/>
        </Grid.Background>

        <TextBox x:Name="txtUsuario" HorizontalAlignment="Left" Margin="260,186,0,0" TextWrapping="Wrap" Text="Usuario" VerticalAlignment="Top" Width="260" FontWeight="Bold" FontFamily="Arial" FontSize="16" Height="31"/>
        <TextBox x:Name="txtPassword" HorizontalAlignment="Left" Margin="260,237,0,0" TextWrapping="Wrap" Text="Contraseña" VerticalAlignment="Top" Width="260" FontWeight="Bold" FontFamily="Arial" FontSize="16" Height="31"/>
        <Button x:Name="btn_regresar" Content="Button" HorizontalAlignment="Left" Margin="588,26,0,0" VerticalAlignment="Top" Click="btn_regresar_Click"/>
        <Button x:Name="btnIngresar" Background="Transparent" BorderBrush="Transparent" Content=""
                Width="260" Height="30"
                Canvas.Left="150" Canvas.Top="150" Click="BtnIngresar_Click" Margin="260,250,283,83"/>

    </Grid>
</Window>
