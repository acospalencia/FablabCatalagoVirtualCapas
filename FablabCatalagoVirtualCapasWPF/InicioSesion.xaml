<Window x:Class="Fablab.esfe.InicioSesion"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Fablab.esfe"
        mc:Ignorable="d"
        Title="InicioSesion" Height="450" Width="800" ResizeMode="NoResize" WindowStartupLocation="CenterScreen" Closed="Window_Closed">
    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="/BgLogin.png"/>
        </Grid.Background>
        <TextBox HorizontalAlignment="Left" Margin="330,146,0,0" TextWrapping="Wrap" Text="Usuario" VerticalAlignment="Top" Width="135" FontWeight="Bold" FontFamily="Arial" FontSize="16" Height="22"/>
        <TextBox HorizontalAlignment="Left" Margin="332,193,0,0" TextWrapping="Wrap" Text="Contraseña" VerticalAlignment="Top" Width="133" FontWeight="Bold" FontFamily="Arial" FontSize="14" Height="24"/>
        <Button x:Name="btn_regresar" Content="Button" HorizontalAlignment="Left" Margin="47,33,0,0" VerticalAlignment="Top" Click="btn_regresar_Click"/>

    </Grid>
</Window>
