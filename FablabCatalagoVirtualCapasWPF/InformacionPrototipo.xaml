        <Label Content="Tiempo necesitado para armarlo:   " HorizontalAlignment="Left" Margin="368,296,0,0" VerticalAlignment="Top" Foreground="#FFF3E8E8" FontWeight="Bold" FontSize="16" FontFamily="Arial"/>
        <Label Content="Ancho: " HorizontalAlignment="Left" Margin="34,296,0,0" VerticalAlignment="Top" Foreground="#FFF3E8E8" FontWeight="Bold" FontSize="16" FontFamily="Arial"/>
        <Image HorizontalAlignment="Left" Height="100" Margin="661,67,0,0" VerticalAlignment="Top" Width="100"/>

    </Grid>
</Window>
