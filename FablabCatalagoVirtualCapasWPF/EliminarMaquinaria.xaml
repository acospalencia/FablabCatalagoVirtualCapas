        <ComboBox HorizontalAlignment="Left" Margin="69,391,0,0" VerticalAlignment="Top" Width="120"/>
        <DataGrid d:ItemsSource="{d:SampleData ItemCount=5}" Margin="316,143,65,125"/>
        <Button x:Name="btnGuardar" Content="" HorizontalAlignment="Left" Margin="460,359,0,0" VerticalAlignment="Top" Width="66" Height="55">
