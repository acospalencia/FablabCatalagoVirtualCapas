        <Label x:Name="txtIdMaterial" Content="Id Material: " HorizontalAlignment="Left" Margin="69,281,0,0" VerticalAlignment="Top" FontWeight="Bold" FontFamily="Arial" FontSize="16" Foreground="White"/>
