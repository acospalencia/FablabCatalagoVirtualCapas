                <ImageBrush ImageSource="Recursos/Botones/BtnGuardar.jpeg"/>
            </Button.Background>
        </Button>
        <Button x:Name="btnEliminar" Content="" HorizontalAlignment="Left" Margin="608,360,0,0" VerticalAlignment="Top" Width="55" Height="53">
            <Button.Background>
                <ImageBrush ImageSource="Recursos/Botones/BtnEliminar.png"/>
            </Button.Background>
        </Button>
        <TextBox x:Name="txtBuscar" HorizontalAlignment="Left" Margin="400,100,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="184" Height="24"/>
